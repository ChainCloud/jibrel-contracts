{
  "extends": "solium:all",
  "plugins": [
    "security"
  ],
  "rules": {
    "lbrace": ["off"],
    "function-order": ["off"],
    "blank-lines": ["off"],
    "quotes": [
      "error",
      "single"
    ],
    "indentation": [
      "error",
      2
    ],
    "arg-overflow": [
      "warning",
      5
    ]
  }
}
